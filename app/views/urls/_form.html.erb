<!--  form_with(url: [@page, @url], model: url, local: true) do |form|  -->
<%= form_with(url: page_url_path, model: url, local: true, id: "urleditform") do |form|  %>
  <% if url.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(url.errors.count, "error") %> prohibited this url from being saved:</h2>

      <ul>
      <% url.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label "名前" %>
    <%= form.text_field :name, id: :url_name %>
  </div>

  <div class="field">
    <%= form.label "URL" %>
    <%= form.text_field :url, id: :url_url %>
  </div>

  <div class="field">
    <!-- <%= form.label :x %> -->
    <%= form.hidden_field :x, id: :url_x %>
  </div>

  <div class="field">
    <!-- <%= form.label :y %> -->
    <%= form.hidden_field :y, id: :url_y %>
  </div>

  <div class="field">
    <!-- <%= form.label :w %> -->
    <%= form.hidden_field :w, id: :url_w %>
  </div>

  <div class="field">
    <!-- <%= form.label :h %> -->
    <%= form.hidden_field :h, id: :url_h %>
  </div>

  <div class="field">
    <!-- <%= form.label :px %> -->
    <%= form.hidden_field :px, id: :url_px %>
  </div>

  <div class="field">
    <!-- <%= form.label :py %> -->
    <%= form.hidden_field :py, id: :url_py %>
  </div>

  <div class="field">
    <!-- <%= form.label :pw %> -->
    <%= form.hidden_field :pw, id: :url_pw %>
  </div>

  <div class="field">
    <!-- <%= form.label :ph %> -->
    <%= form.hidden_field :ph, id: :url_ph %>
  </div>

  <div class="field">
    <!-- <%= form.label :zoom %> -->
    <%= form.hidden_field :zoom, id: :url_zoom %>
  </div>

  <div class="field">
    <!-- <%= form.label :autoup %> -->
    <%= form.hidden_field :autoup, id: :url_autoup %>
  </div>

  <div class="field">
    <!-- <%= form.label :captime %> -->
    <%= form.hidden_field :captime, id: :url_captime %>
  </div>

  <div class="field">
    <!-- <%= form.label :hour %> -->
    <%= form.hidden_field :hour, id: :url_hour %>
  </div>

  <div class="field">
    <!-- <%= form.label :minute %> -->
    <%= form.hidden_field :minute, id: :url_minute %>
  </div>

  <div class="field">
    <!-- <%= form.label :visible %> -->
    <%= form.hidden_field :visible, id: :url_visible %>
  </div>

  <div class="field">
    <!-- <%= form.label :user_id %> -->
    <%= form.hidden_field :user_id, id: :url_user_id, value: current_user.id %>
  </div>

  <div class="field">
    <!-- <%= form.label :page_id %> -->
    <%= form.hidden_field :page_id, id: :url_page_id, value: params[:page_id] %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

<p id="notice"><%= notice %></p>

<h1>Urls of Page <%= @page.name %> (page.id = <%= @page.id %>) </h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>X</th>
      <th>Y</th>
      <th>W</th>
      <th>H</th>
      <th>Px</th>
      <th>Py</th>
      <th>Pw</th>
      <th>Ph</th>
      <th>Zoom</th>
      <th>Autoup</th>
      <th>Captime</th>
      <th>Hour</th>
      <th>Minute</th>
      <th>Visible</th>
      <th>User</th>
      <th>Page</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |url| %>
      <tr>
        <td><%= url.name %></td>
        <td><%= url.url %></td>
        <td><%= url.x %></td>
        <td><%= url.y %></td>
        <td><%= url.w %></td>
        <td><%= url.h %></td>
        <td><%= url.px %></td>
        <td><%= url.py %></td>
        <td><%= url.pw %></td>
        <td><%= url.ph %></td>
        <td><%= url.zoom %></td>
        <td><%= url.autoup %></td>
        <td><%= url.captime %></td>
        <td><%= url.hour %></td>
        <td><%= url.minute %></td>
        <td><%= url.visible %></td>
        <td><%= url.user_id %></td>
        <td><%= url.page_id %></td>
        <td><%= link_to 'Show', url %></td>
        <td><%= link_to 'Edit', edit_page_url_path(@page, url) %></td>
        <td><%= link_to 'Destroy', url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to Page', page_path(@page) %>
<%= link_to 'Add New Url', new_page_url_path %>
